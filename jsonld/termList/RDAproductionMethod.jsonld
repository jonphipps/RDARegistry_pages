  "@graph": [
    {
      "@id": "http://rdaregistry.info/termList/RDAproductionMethod",
      "@type": "skos:ConceptScheme",
      "dc:title": "RDA Production Method"
    },
    {
      "@id": "http://rdaregistry.info/termList/RDAproductionMethod/1001",
      "@type": "skos:Concept",
      "reg:identifier": {
        "@id": "http://api.metadataregistry.org/concepts/424"
      },
      "reg:status": {
        "@id": "http://metadataregistry.org/uri/RegStatus/1001"
      },
      "skos:definition": {
        "@language": "en",
        "@value": "A production method consisting of prints made on light-sensitized surfaces that produce blue images on neutral backgrounds."
      },
      "skos:inScheme": {
        "@id": "http://rdaregistry.info/termList/RDAproductionMethod"
      },
      "skos:prefLabel": {
        "@language": "en",
        "@value": "blueline"
      },
      "skos:related": {
        "@id": "http://rdaregistry.info/termList/RDAproductionMethod/1002"
      }
    },
    {
      "@id": "http://rdaregistry.info/termList/RDAproductionMethod/1003",
      "@type": "skos:Concept",
      "reg:identifier": {
        "@id": "http://api.metadataregistry.org/concepts/426"
      },
      "reg:status": {
        "@id": "http://metadataregistry.org/uri/RegStatus/1001"
      },
      "skos:definition": {
        "@language": "en",
        "@value": "A production method consisting of a type of photolithography in which the printing plate is prepared using a bichromate process. Unhardened gelatin areas hold water and thus resist greasy ink; hardened areas accept ink and hold it in the characteristic wormlike pattern of cracks."
      },
      "skos:inScheme": {
        "@id": "http://rdaregistry.info/termList/RDAproductionMethod"
      },
      "skos:prefLabel": {
        "@language": "en",
        "@value": "collotype"
      }
    },
    {
      "@id": "http://metadataregistry.org/uri/RegStatus/1001",
      "@type": "skos:Concept",
      "skos:prefLabel": {
        "@language": "en",
        "@value": "Published"
      }
    },
    {
      "@id": "http://rdaregistry.info/termList/RDAproductionMethod/1002",
      "@type": "skos:Concept",
      "reg:identifier": {
        "@id": "http://api.metadataregistry.org/concepts/425"
      },
      "reg:status": {
        "@id": "http://metadataregistry.org/uri/RegStatus/1001"
      },
      "skos:definition": {
        "@language": "en",
        "@value": "A production method consisting of a photographic process using iron salts and producing an image in Prussian blue. Blueprints are reproductive prints of architectural plans, maps, mechanical drawings, and other technical drawings, characterized by having white images on blue backgrounds."
      },
      "skos:inScheme": {
        "@id": "http://rdaregistry.info/termList/RDAproductionMethod"
      },
      "skos:prefLabel": {
        "@language": "en",
        "@value": "blueprint"
      },
      "skos:related": {
        "@id": "http://rdaregistry.info/termList/RDAproductionMethod/1001"
      }
    },
    {
      "@id": "http://rdaregistry.info/termList/RDAproductionMethod/1010",
      "@type": "skos:Concept",
      "reg:identifier": {
        "@id": "http://api.metadataregistry.org/concepts/433"
      },
      "reg:status": {
        "@id": "http://metadataregistry.org/uri/RegStatus/1002"
      },
      "skos:inScheme": {
        "@id": "http://rdaregistry.info/termList/RDAproductionMethod"
      },
      "skos:prefLabel": {
        "@language": "en",
        "@value": "print"
      }
    },
    {
      "@id": "http://rdaregistry.info/termList/RDAproductionMethod/1011",
      "@type": "skos:Concept",
      "reg:identifier": {
        "@id": "http://api.metadataregistry.org/concepts/434"
      },
      "reg:status": {
        "@id": "http://metadataregistry.org/uri/RegStatus/1001"
      },
      "skos:definition": {
        "@language": "en",
        "@value": "A production method consisting of copies made by light-sensitive processes, often the diazo process, usually of line drawings, in which black or coloured lines appear on a white background."
      },
      "skos:inScheme": {
        "@id": "http://rdaregistry.info/termList/RDAproductionMethod"
      },
      "skos:prefLabel": {
        "@language": "en",
        "@value": "white print"
      }
    },
    {
      "@id": "http://rdaregistry.info/termList/RDAproductionMethod/1012",
      "@type": "skos:Concept",
      "reg:identifier": {
        "@id": "http://api.metadataregistry.org/concepts/435"
      },
      "reg:status": {
        "@id": "http://metadataregistry.org/uri/RegStatus/1002"
      },
      "skos:inScheme": {
        "@id": "http://rdaregistry.info/termList/RDAproductionMethod"
      },
      "skos:prefLabel": {
        "@language": "en",
        "@value": "woodcut"
      }
    },
    {
      "@id": "http://rdaregistry.info/termList/RDAproductionMethod/1006",
      "@type": "skos:Concept",
      "reg:identifier": {
        "@id": "http://api.metadataregistry.org/concepts/429"
      },
      "reg:status": {
        "@id": "http://metadataregistry.org/uri/RegStatus/1001"
      },
      "skos:definition": {
        "@language": "en",
        "@value": "Production method consisting of an intaglio process in which the design is worked into an acid-resistant substance coating the metal printing plate; the plate is then exposed to acid, which etches the plate where the metal is exposed, to create lines and dark areas."
      },
      "skos:inScheme": {
        "@id": "http://rdaregistry.info/termList/RDAproductionMethod"
      },
      "skos:prefLabel": {
        "@language": "en",
        "@value": "etching"
      },
      "skos:related": {
        "@id": "http://rdaregistry.info/termList/RDAproductionMethod/1005"
      }
    },
    {
      "@id": "http://rdaregistry.info/termList/RDAproductionMethod/1007",
      "@type": "skos:Concept",
      "reg:identifier": {
        "@id": "http://api.metadataregistry.org/concepts/430"
      },
      "reg:status": {
        "@id": "http://metadataregistry.org/uri/RegStatus/1002"
      },
      "skos:inScheme": {
        "@id": "http://rdaregistry.info/termList/RDAproductionMethod"
      },
      "skos:prefLabel": {
        "@language": "en",
        "@value": "lithograph"
      }
    },
    {
      "@id": "http://rdaregistry.info/termList/RDAproductionMethod/1004",
      "@type": "skos:Concept",
      "reg:identifier": {
        "@id": "http://api.metadataregistry.org/concepts/427"
      },
      "reg:status": {
        "@id": "http://metadataregistry.org/uri/RegStatus/1001"
      },
      "skos:definition": {
        "@language": "en",
        "@value": "A production method consisting of exposure in a camera of a silver-coated copper plate that is subsequently developed, usually using mercury vapour, and fixed with salt to create a positive image."
      },
      "skos:inScheme": {
        "@id": "http://rdaregistry.info/termList/RDAproductionMethod"
      },
      "skos:prefLabel": {
        "@language": "en",
        "@value": "daguerreotype"
      }
    },
    {
      "@id": "http://rdaregistry.info/termList/RDAproductionMethod/1005",
      "@type": "skos:Concept",
      "reg:identifier": {
        "@id": "http://api.metadataregistry.org/concepts/428"
      },
      "reg:status": {
        "@id": "http://metadataregistry.org/uri/RegStatus/1001"
      },
      "skos:definition": {
        "@language": "en",
        "@value": "A production method consisting of creating marks on the surface of a hard material, such as metal or glass, by incising with a sharp tool. In printing, the intaglio process in which the design is incised into a printing plate, usually a flat copper plate, with the aid of a graver or burin that is held in the palm of the hand and pushed against the copper to cut lines comprising V-shaped grooves. The plate is then inked up, wiped so that ink is retained in the grooves and then forced out under the pressure of the printing process to create lines on the paper. The technique was first developed in the early 15th century in Germany. Historically, \"engraving\" has sometimes been used incorrectly to refer to all printmaking processes, particularly any process employing printing plates."
      },
      "skos:inScheme": {
        "@id": "http://rdaregistry.info/termList/RDAproductionMethod"
      },
      "skos:prefLabel": {
        "@language": "en",
        "@value": "engraving"
      },
      "skos:related": {
        "@id": "http://rdaregistry.info/termList/RDAproductionMethod/1006"
      }
    },
    {
      "@id": "http://metadataregistry.org/uri/RegStatus/1002",
      "@type": "skos:Concept",
      "skos:prefLabel": {
        "@language": "en",
        "@value": "New-Proposed"
      }
    },
    {
      "@id": "http://rdaregistry.info/termList/RDAproductionMethod/1008",
      "@type": "skos:Concept",
      "reg:identifier": {
        "@id": "http://api.metadataregistry.org/concepts/431"
      },
      "reg:status": {
        "@id": "http://metadataregistry.org/uri/RegStatus/1001"
      },
      "skos:definition": {
        "@language": "en",
        "@value": "A production method consisting of a macroform photoreproduction produced directly on opaque material by radiant energy through contact or projection."
      },
      "skos:inScheme": {
        "@id": "http://rdaregistry.info/termList/RDAproductionMethod"
      },
      "skos:prefLabel": {
        "@language": "en",
        "@value": "photocopy"
      }
    },
    {
      "@id": "http://rdaregistry.info/termList/RDAproductionMethod/1009",
      "@type": "skos:Concept",
      "reg:identifier": {
        "@id": "http://api.metadataregistry.org/concepts/432"
      },
      "reg:status": {
        "@id": "http://metadataregistry.org/uri/RegStatus/1001"
      },
      "skos:definition": {
        "@language": "en",
        "@value": "A production method using a photomechanical process to prepare chemically etched printing plates."
      },
      "skos:inScheme": {
        "@id": "http://rdaregistry.info/termList/RDAproductionMethod"
      },
      "skos:prefLabel": {
        "@language": "en",
        "@value": "photoengraving"
      }
    },
    {
      "@id": "http://rdaregistry.info/termList/RDAproductionMethod/1014",
      "@type": "skos:Concept",
      "reg:identifier": {
        "@id": "http://api.metadataregistry.org/concepts/1137"
      },
      "reg:status": {
        "@id": "http://metadataregistry.org/uri/RegStatus/1001"
      },
      "skos:definition": {
        "@language": "en",
        "@value": "A production method consisting of an intaglio method in which the metal printing plate is prepared using a bichromate process, leaving a gelatin resist of varying thickness. The plate is etched to form cells of varying depth able to hold different amounts of ink."
      },
      "skos:inScheme": {
        "@id": "http://rdaregistry.info/termList/RDAproductionMethod"
      },
      "skos:prefLabel": {
        "@language": "en",
        "@value": "photogravure"
      }
    }
  ]
}
