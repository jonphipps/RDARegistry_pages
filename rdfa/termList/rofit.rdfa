<div xmlns="http://www.w3.org/1999/xhtml"
  prefix="
    reg: http://metadataregistry.org/uri/profile/RegAp/
    rdf: http://www.w3.org/1999/02/22-rdf-syntax-ns#
    skos: http://www.w3.org/2004/02/skos/core#
    dc: http://purl.org/dc/elements/1.1/
    rdfs: http://www.w3.org/2000/01/rdf-schema#"
  >
  <div typeof="skos:Concept" about="http://rdaregistry.info/termList/rofit/T1002">
    <div rel="reg:identifier" resource="http://api.metadataregistry.org/concepts/6954"></div>
    <div rel="reg:status">
      <div typeof="skos:Concept" about="http://metadataregistry.org/uri/RegStatus/1001">
        <div property="skos:prefLabel" xml:lang="en" content="Published"></div>
      </div>
    </div>
    <div rel="skos:inScheme">
      <div typeof="skos:ConceptScheme" about="http://rdaregistry.info/termList/rofit">
        <div property="dc:title" content="ROF Intermediation Tool"></div>
      </div>
    </div>
    <div property="skos:definition" xml:lang="en" content="An instrument that magnifies objects by means of a lens or lenses so as to reveal details invisible to the naked eye."></div>
    <div property="skos:prefLabel" xml:lang="en" content="microscope"></div>
  </div>
  <div typeof="skos:Concept" about="http://rdaregistry.info/termList/rofit/T1008">
    <div rel="skos:inScheme" resource="http://rdaregistry.info/termList/rofit"></div>
    <div rel="reg:status" resource="http://metadataregistry.org/uri/RegStatus/1001"></div>
    <div property="skos:prefLabel" xml:lang="en" content="not required"></div>
    <div rel="reg:identifier" resource="http://api.metadataregistry.org/concepts/6960"></div>
    <div property="skos:definition" xml:lang="en" content="No intermediation tool is required (i.e., the content of the resource is directly and adequately perceivable through one or more of the human senses)."></div>
  </div>
  <div typeof="skos:Concept" about="http://rdaregistry.info/termList/rofit/T1001">
    <div property="skos:definition" xml:lang="en" content="A device that magnifies microforms for reading with the unaided eye."></div>
    <div rel="skos:inScheme" resource="http://rdaregistry.info/termList/rofit"></div>
    <div rel="reg:identifier" resource="http://api.metadataregistry.org/concepts/6953"></div>
    <div rel="reg:status" resource="http://metadataregistry.org/uri/RegStatus/1001"></div>
    <div property="skos:prefLabel" xml:lang="en" content="microform reader"></div>
  </div>
  <div typeof="skos:Concept" about="http://rdaregistry.info/termList/rofit/T1006">
    <div rel="reg:status" resource="http://metadataregistry.org/uri/RegStatus/1001"></div>
    <div property="skos:prefLabel" xml:lang="en" content="audiovisual player"></div>
    <div rel="skos:inScheme" resource="http://rdaregistry.info/termList/rofit"></div>
    <div property="skos:definition" xml:lang="en" content="A device designed to play audiovisual recordings."></div>
    <div rel="reg:identifier" resource="http://api.metadataregistry.org/concepts/6958"></div>
  </div>
  <div typeof="skos:Concept" about="http://rdaregistry.info/termList/rofit/T1007">
    <div rel="reg:identifier" resource="http://api.metadataregistry.org/concepts/6959"></div>
    <div rel="skos:inScheme" resource="http://rdaregistry.info/termList/rofit"></div>
    <div property="skos:prefLabel" xml:lang="en" content="computer"></div>
    <div rel="reg:status" resource="http://metadataregistry.org/uri/RegStatus/1001"></div>
    <div property="skos:definition" xml:lang="en" content="A device designed to perform prescribed sequences of operations on electronic data."></div>
  </div>
  <div typeof="skos:Concept" about="http://rdaregistry.info/termList/rofit/T1003">
    <div rel="skos:inScheme" resource="http://rdaregistry.info/termList/rofit"></div>
    <div property="skos:prefLabel" xml:lang="en" content="projector"></div>
    <div rel="reg:identifier" resource="http://api.metadataregistry.org/concepts/6955"></div>
    <div property="skos:definition" xml:lang="en" content="An optical device containing a light source and lens system for projecting an image on a screen or other surface."></div>
    <div rel="reg:status" resource="http://metadataregistry.org/uri/RegStatus/1001"></div>
  </div>
  <div typeof="skos:Concept" about="http://rdaregistry.info/termList/rofit/T1004">
    <div property="skos:definition" xml:lang="en" content="An optical device with two lenses enabling each eye to see a separate image of essentially the same content to give the effect of three-dimensions."></div>
    <div property="skos:prefLabel" xml:lang="en" content="stereoscope"></div>
    <div rel="skos:inScheme" resource="http://rdaregistry.info/termList/rofit"></div>
    <div rel="reg:identifier" resource="http://api.metadataregistry.org/concepts/6956"></div>
    <div rel="reg:status" resource="http://metadataregistry.org/uri/RegStatus/1001"></div>
  </div>
  <div typeof="skos:Concept" about="http://rdaregistry.info/termList/rofit/T1005">
    <div rel="reg:identifier" resource="http://api.metadataregistry.org/concepts/6957"></div>
    <div rel="reg:status" resource="http://metadataregistry.org/uri/RegStatus/1001"></div>
    <div property="skos:definition" xml:lang="en" content="A device designed to play audio recordings."></div>
    <div rel="skos:inScheme" resource="http://rdaregistry.info/termList/rofit"></div>
    <div property="skos:prefLabel" xml:lang="en" content="audio player"></div>
  </div>
</div>
