<div xmlns="http://www.w3.org/1999/xhtml"
  prefix="
    rdf: http://www.w3.org/1999/02/22-rdf-syntax-ns#
    skos: http://www.w3.org/2004/02/skos/core#
    reg: http://metadataregistry.org/uri/profile/RegAp/
    dc: http://purl.org/dc/elements/1.1/
    rdfs: http://www.w3.org/2000/01/rdf-schema#"
  >
  <div typeof="skos:Concept" about="http://rdaregistry.info/termList/scale/1001">
    <div property="skos:prefLabel" xml:lang="en" content="Scale differs"></div>
    <div rel="reg:identifier" resource="http://api.metadataregistry.org/concepts/1141"></div>
    <div rel="skos:inScheme">
      <div typeof="skos:ConceptScheme" about="http://rdaregistry.info/termList/scale">
        <div property="dc:title" content="RDA Scale"></div>
      </div>
    </div>
    <div rel="reg:status">
      <div typeof="skos:Concept" about="http://metadataregistry.org/uri/RegStatus/1001">
        <div property="skos:prefLabel" xml:lang="en" content="Published"></div>
      </div>
    </div>
    <div property="skos:definition" xml:lang="en" content="Scale designation for a resource consisting of more than one image, map, etc., with different scales."></div>
  </div>
  <div typeof="skos:Concept" about="http://rdaregistry.info/termList/scale/1004">
    <div property="skos:prefLabel" xml:lang="en" content="Not drawn to scale"></div>
    <div rel="reg:status" resource="http://metadataregistry.org/uri/RegStatus/1001"></div>
    <div rel="reg:identifier" resource="http://api.metadataregistry.org/concepts/3392"></div>
    <div rel="skos:inScheme" resource="http://rdaregistry.info/termList/scale"></div>
    <div property="skos:definition" xml:lang="en" content="Scale designation for a still image or three-dimensional form that is not to scale."></div>
  </div>
  <div typeof="skos:Concept" about="http://rdaregistry.info/termList/scale/1002">
    <div property="skos:definition" xml:lang="en" content="Scale designation for a resource when no scale can be determined."></div>
    <div property="skos:prefLabel" xml:lang="en" content="Scale not given"></div>
    <div rel="reg:status" resource="http://metadataregistry.org/uri/RegStatus/1001"></div>
    <div rel="reg:identifier" resource="http://api.metadataregistry.org/concepts/1142"></div>
    <div rel="skos:inScheme" resource="http://rdaregistry.info/termList/scale"></div>
  </div>
  <div typeof="skos:Concept" about="http://rdaregistry.info/termList/scale/1003">
    <div rel="skos:inScheme" resource="http://rdaregistry.info/termList/scale"></div>
    <div rel="reg:identifier" resource="http://api.metadataregistry.org/concepts/1143"></div>
    <div rel="reg:status" resource="http://metadataregistry.org/uri/RegStatus/1001"></div>
    <div property="skos:prefLabel" xml:lang="en" content="Scale varies"></div>
    <div property="skos:definition" xml:lang="en" content="Scale designation for a resource whose scale is variable across the resource, when the range of values cannot be determined."></div>
  </div>
</div>
