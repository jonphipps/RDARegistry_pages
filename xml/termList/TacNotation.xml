      <skos:inScheme rdf:resource="http://rdaregistry.info/termList/TacNotation/"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://api.metadataregistry.org/concepts/1026"/>
      <skos:prefLabel xml:lang="en">tactile graphic</skos:prefLabel>
      <skos:definition xml:lang="en">A form of tactile notation using a raised version of a print graphic.</skos:definition>
      <skos:prefLabel xml:lang="de">taktile Grafik</skos:prefLabel>
