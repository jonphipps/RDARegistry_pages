<!-- Scheme: RDA Form of Tactile Notation -->
    <skos:ConceptScheme rdf:about="http://rdaregistry.info/termList/TacNotation/">
        <dc:title>RDA Form of Tactile Notation</dc:title>
    </skos:ConceptScheme>

<!-- Concept: braille code  -->
    <skos:Concept rdf:about="http://rdaregistry.info/termList/TacNotation/1001" xml:lang="en">
      <skos:inScheme rdf:resource="http://rdaregistry.info/termList/TacNotation/"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://api.metadataregistry.org/concepts/708"/>
      <skos:prefLabel xml:lang="en">braille code</skos:prefLabel>
      <skos:definition xml:lang="en">A form of tactile notation for text using embossed characters formed by raised dots in six-dot cells.</skos:definition>
      <skos:prefLabel xml:lang="de">Brailleschrift</skos:prefLabel>
      <skos:definition xml:lang="de">Eine Form der taktilen Notation für Text, unter Verwendung von geprägten Zeichen, die durch erhöhte Punkte in 6-Punkt-Zellen gebildet werden.</skos:definition>
      <skos:prefLabel xml:lang="fr">braille</skos:prefLabel>
      <skos:definition xml:lang="fr">Forme de notation tactile du texte utilisant des caractères en relief formés de points saillants par cellules de six.</skos:definition>
      <skos:prefLabel xml:lang="es">código Braille</skos:prefLabel>
      <skos:definition xml:lang="es">Forma de notación táctil para textos, que utiliza caracteres grabados en relieve formados mediante puntos realzados en celdas de seis puntos.</skos:definition>
      <skos:prefLabel xml:lang="zh">布莱叶盲文编码</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="zh">布莱叶盲文编码</rdakit:toolkitLabel>
      <rdakit:toolkitLabel xml:lang="en">braille code</rdakit:toolkitLabel>
      <skos:definition xml:lang="zh">一种文本的触摸标记形式，以六点为一个单位、采用点状的凸起记录文字。</skos:definition>
      <rdakit:toolkitDefinition xml:lang="zh">一种文本的触摸标记形式，以六点为一个单位、采用点状的凸起记录文字。</rdakit:toolkitDefinition>
      <rdakit:toolkitDefinition xml:lang="en">A form of tactile notation for text using embossed characters formed by raised dots in six-dot cells.</rdakit:toolkitDefinition>
      <rdakit:toolkitLabel xml:lang="fr">braille</rdakit:toolkitLabel>
      <rdakit:toolkitDefinition xml:lang="fr">Forme de notation tactile du texte utilisant des caractères en relief formés de points saillants par cellules de six.</rdakit:toolkitDefinition>
      <rdakit:toolkitLabel xml:lang="es">código Braille</rdakit:toolkitLabel>
      <rdakit:toolkitDefinition xml:lang="es">Forma de notación táctil para textos, que utiliza caracteres grabados en relieve formados mediante puntos realzados en celdas de seis puntos.</rdakit:toolkitDefinition>
    </skos:Concept>
<!-- Concept: computing braille code  -->
    <skos:Concept rdf:about="http://rdaregistry.info/termList/TacNotation/1002" xml:lang="en">
      <skos:inScheme rdf:resource="http://rdaregistry.info/termList/TacNotation/"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://api.metadataregistry.org/concepts/709"/>
      <skos:prefLabel xml:lang="en">computing braille code</skos:prefLabel>
      <skos:definition xml:lang="en">A form of tactile notation for computer related materials which enables the representation of symbols and ASCII code.</skos:definition>
      <skos:altLabel xml:lang="en">computer braille code</skos:altLabel>
      <skos:prefLabel xml:lang="de">Computer-Braille</skos:prefLabel>
      <skos:definition xml:lang="de">Eine Form der taktilen Notation für Computermaterialien, die die Repräsentation von Symbolen und ASCII-Code ermöglicht.</skos:definition>
      <skos:definition xml:lang="fr">braille informatique</skos:definition>
      <skos:definition xml:lang="fr">Forme de notation tactile pour les documents touchant l’informatique qui permet de représenter des symboles et le code ASCII. </skos:definition>
      <skos:altLabel xml:lang="fr">Braille 8 points</skos:altLabel>
      <skos:prefLabel xml:lang="es">código Braille para computación</skos:prefLabel>
      <skos:definition xml:lang="es">Forma de notación táctil para materiales relacionados con computadora que permite la representación de símbolos y código ASCII.</skos:definition>
      <skos:prefLabel xml:lang="zh">计算布莱叶盲文编码</skos:prefLabel>
      <skos:altLabel xml:lang="zh">计算机布莱叶盲文编码</skos:altLabel>
      <rdakit:toolkitLabel xml:lang="zh">计算布莱叶盲文编码</rdakit:toolkitLabel>
      <rdakit:toolkitLabel xml:lang="en">computing braille code</rdakit:toolkitLabel>
      <skos:definition xml:lang="zh">一种用于计算机相关资料的触摸标记形式，它能显示符号和ASCII代码。</skos:definition>
      <rdakit:toolkitDefinition xml:lang="zh">一种用于计算机相关资料的触摸标记形式，它能显示符号和ASCII代码。</rdakit:toolkitDefinition>
      <rdakit:toolkitDefinition xml:lang="en">A form of tactile notation for computer related materials which enables the representation of symbols and ASCII code.</rdakit:toolkitDefinition>
      <rdakit:toolkitDefinition xml:lang="fr">braille informatique</rdakit:toolkitDefinition>
      <skos:scopeNote xml:lang="en">Also called Computer braille code.</skos:scopeNote>
      <skos:altLabel xml:lang="es">código Braille para computadora</skos:altLabel>
      <rdakit:toolkitLabel xml:lang="es">código Braille para computación</rdakit:toolkitLabel>
      <rdakit:toolkitDefinition xml:lang="es">Forma de notación táctil para materiales relacionados con computadora que permite la representación de símbolos y código ASCII.</rdakit:toolkitDefinition>
    </skos:Concept>
<!-- Concept: mathematics braille code  -->
    <skos:Concept rdf:about="http://rdaregistry.info/termList/TacNotation/1003" xml:lang="en">
      <skos:inScheme rdf:resource="http://rdaregistry.info/termList/TacNotation/"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://api.metadataregistry.org/concepts/710"/>
      <skos:prefLabel xml:lang="en">mathematics braille code</skos:prefLabel>
      <skos:definition xml:lang="en">A form of tactile notation used to transcribe mathematical and scientific information.</skos:definition>
      <skos:prefLabel xml:lang="de">Braille-Mathematikschrift</skos:prefLabel>
      <skos:definition xml:lang="de">Eine Form der taktilen Notation, die verwendet wird, um mathematische und naturwissenschaftliche Informationen zu übertragen.</skos:definition>
      <skos:prefLabel xml:lang="fr">braille mathématique</skos:prefLabel>
      <skos:definition xml:lang="fr">Forme de notation tactile employée pour transcrire de l’information mathématique et scientifique.</skos:definition>
      <skos:prefLabel xml:lang="es">código Braille para matemáticas</skos:prefLabel>
      <skos:definition xml:lang="es">Forma de notación táctil utilizada para transcribir información matemática y científica.</skos:definition>
      <skos:prefLabel xml:lang="zh">数学布莱叶盲文编码</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="zh">数学布莱叶盲文编码</rdakit:toolkitLabel>
      <rdakit:toolkitLabel xml:lang="en">mathematics braille code</rdakit:toolkitLabel>
      <skos:definition xml:lang="zh">一种用于转录数学和科学信息的触摸标记形式。</skos:definition>
      <rdakit:toolkitDefinition xml:lang="zh">一种用于转录数学和科学信息的触摸标记形式。</rdakit:toolkitDefinition>
      <rdakit:toolkitDefinition xml:lang="en">A form of tactile notation used to transcribe mathematical and scientific information.</rdakit:toolkitDefinition>
      <rdakit:toolkitLabel xml:lang="fr">braille mathématique</rdakit:toolkitLabel>
      <rdakit:toolkitDefinition xml:lang="fr">Forme de notation tactile employée pour transcrire de l’information mathématique et scientifique.</rdakit:toolkitDefinition>
      <rdakit:toolkitLabel xml:lang="es">código Braille para matemáticas</rdakit:toolkitLabel>
      <rdakit:toolkitDefinition xml:lang="es">Forma de notación táctil utilizada para transcribir información matemática y científica.</rdakit:toolkitDefinition>
    </skos:Concept>
<!-- Concept: Moon code  -->
    <skos:Concept rdf:about="http://rdaregistry.info/termList/TacNotation/1004" xml:lang="en">
      <skos:inScheme rdf:resource="http://rdaregistry.info/termList/TacNotation/"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://api.metadataregistry.org/concepts/711"/>
      <skos:prefLabel xml:lang="en">Moon code</skos:prefLabel>
      <skos:definition xml:lang="en">A form of tactile notation based on simplified letter forms.</skos:definition>
      <skos:prefLabel xml:lang="de">Moonschrift</skos:prefLabel>
      <skos:definition xml:lang="de">Eine Form der taktilen Notation, die auf vereinfachten Buchstabenformen basiert.</skos:definition>
      <skos:prefLabel xml:lang="fr">code Moon</skos:prefLabel>
      <skos:definition xml:lang="fr">Forme de notation tactile basée sur des formes simplifiées de lettres.</skos:definition>
      <skos:prefLabel xml:lang="es">código Moon</skos:prefLabel>
      <skos:definition xml:lang="es">Forma de notación táctil basada en formas de letras simplificadas.</skos:definition>
      <skos:prefLabel xml:lang="zh">蒙恩编码</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="zh">蒙恩编码</rdakit:toolkitLabel>
      <rdakit:toolkitLabel xml:lang="en">Moon code</rdakit:toolkitLabel>
      <skos:definition xml:lang="zh">基于简化字母形式的盲文标记形式。</skos:definition>
      <rdakit:toolkitDefinition xml:lang="zh">基于简化字母形式的盲文标记形式。</rdakit:toolkitDefinition>
      <rdakit:toolkitDefinition xml:lang="en">A form of tactile notation based on simplified letter forms.</rdakit:toolkitDefinition>
      <rdakit:toolkitLabel xml:lang="fr">code Moon</rdakit:toolkitLabel>
      <rdakit:toolkitDefinition xml:lang="fr">Forme de notation tactile basée sur des formes simplifiées de lettres.</rdakit:toolkitDefinition>
      <rdakit:toolkitLabel xml:lang="es">código Moon</rdakit:toolkitLabel>
      <rdakit:toolkitDefinition xml:lang="es">Forma de notación táctil basada en formas de letras simplificadas.</rdakit:toolkitDefinition>
    </skos:Concept>
<!-- Concept: music braille code  -->
    <skos:Concept rdf:about="http://rdaregistry.info/termList/TacNotation/1005" xml:lang="en">
      <skos:inScheme rdf:resource="http://rdaregistry.info/termList/TacNotation/"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://api.metadataregistry.org/concepts/712"/>
      <skos:prefLabel xml:lang="en">music braille code</skos:prefLabel>
      <skos:definition xml:lang="en">A form of tactile notation for music using braille cells.</skos:definition>
      <skos:prefLabel xml:lang="de">Braille-Musikschrift</skos:prefLabel>
      <skos:definition xml:lang="de">Eine Form der taktilen Notation für Musik unter Verwendung von Braille-Zellen.</skos:definition>
      <skos:prefLabel xml:lang="fr">braille musical</skos:prefLabel>
      <skos:definition xml:lang="fr">Forme de notation tactile pour la musique qui emploie des cellules braille.</skos:definition>
      <skos:prefLabel xml:lang="es">código Braille para música</skos:prefLabel>
      <skos:definition xml:lang="es">Forma de notación táctil para música usando células braille.</skos:definition>
      <skos:prefLabel xml:lang="zh">乐谱布莱叶盲文编码</skos:prefLabel>
